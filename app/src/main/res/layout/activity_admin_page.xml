<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="app.start.lonewolf.chatapp.AdminPage">

    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/appBarLayout2">

    <include layout="@layout/app_bar_layout"
        android:id="@+id/lay_admin_ques"/>

        <android.support.design.widget.TabLayout
            android:id="@+id/TabQuestions"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:tabSelectedTextColor="@color/common_google_signin_btn_text_dark_pressed"
            app:tabTextColor="@color/white"></android.support.design.widget.TabLayout>

    </android.support.design.widget.AppBarLayout>

    <android.support.v4.view.ViewPager
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/appBarLayout2"
        android:id="@+id/vpQues"></android.support.v4.view.ViewPager>

</RelativeLayout>
